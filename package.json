{
  "name": "sparks",
  "description": "A Sparked by Resistance dice-rolling bot for Discord",
  "author": "Kavita Poduri kavi.pdri@gmail.com (http://github.com/yrgirlkv)",
  "packageManager": "yarn@3.2.2",
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "discord.js": "^14.1.1"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node build/index.js",
    "test": "concurrently \"tsc -w\" \"nodemon build/index.js\"",
    "deploy:commmands": "node build/deploy-commands.js",
    "format": "prettier --write .",
    "prepare": "husky install",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "singleQuote": true
  }
}
